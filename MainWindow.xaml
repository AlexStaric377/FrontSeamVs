                                                                  Text="{Binding gender,UpdateSourceTrigger=PropertyChanged }"    HorizontalAlignment="Left" Margin="60,0,0,0" VerticalAlignment="Center" IsEnabled="False"   >
                                                    </ComboBox>
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab8" Height="50" Grid.Column="1" Grid.Row="4" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width ="110*" />
                                                        <ColumnDefinition Width ="100*" />
                                                    </Grid.ColumnDefinitions>
