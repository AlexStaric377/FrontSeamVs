    <Window x:Class="FrontSeam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:FrontSeam" d:DataContext ="{d:DesignInstance Type=local:MapOpisViewModel}"
        mc:Ignorable="d"
        Title="Диференційна діагностика стану нездужання людини-SEAM" Name="WindowMain" Height="700" Width="800" Background="White"  Language="ru-RU" ShowInTaskbar="True" WindowStyle="SingleBorderWindow" WindowStartupLocation="CenterScreen" ResizeMode="CanResize">
    <Window.Resources>
        <Style x:Key="DGridCentAl" TargetType="{x:Type DataGridColumnHeader}">
            <Setter Property="Control.HorizontalContentAlignment" Value="Center"/>
        </Style>
    </Window.Resources>
    <Window.DataContext>
        <local:MapOpisViewModel/>
    </Window.DataContext>
    <Viewbox>
        <Grid Width="800" Height="700" >
            <Grid.RowDefinitions>

                <RowDefinition Height="600*" />
                <RowDefinition Height="60*"/>
                <RowDefinition Height="30*" />
            </Grid.RowDefinitions>

            <Grid Grid.Row="0" Height="600" x:Name="GridMain" >
                <!--<Border >-->
                <!--<i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseLeftButtonDown">
                            <i:InvokeCommandAction Command="{Binding ElementName=GridMain, Path=DataContext.MainTabControl}" CommandParameter=""/>
                        </i:EventTrigger>
                    </i:Interaction.Triggers>-->

                <TabControl x:Name="ControlMain" TabStripPlacement="Top" BorderThickness="0,0,0,0" Background="White" VerticalAlignment="Top" Height="600" Width="800">
                    <TabControl.Resources>
                        <Style TargetType="TabItem">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="TabItem">
                                        <Grid Name="Panel">
                                            <ContentPresenter x:Name="ContentSite"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    ContentSource="Header"
                                    Margin="10,2"/>
                                        </Grid>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter TargetName="Panel" Property="Background" Value="#FFC8E0F7" />
                                            </Trigger>
                                            <Trigger Property="IsSelected" Value="False">
                                                <Setter TargetName="Panel" Property="Background" Value="White" />
                                            </Trigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </TabControl.Resources>

                    <TabItem Header="   Приймальня" Name="Gues"   FontFamily="Microsoft Sans Serif" Background="#FFC8E0F7"
                             FontSize="14" Height="50" Width="150" BorderThickness="0,0,0,0"  >

                        <TabControl  x:Name="ControlGuest"  HorizontalAlignment="Left" Height="550" VerticalAlignment="Top"  Width="800"  BorderThickness="0,0,0,0" TabStripPlacement="Left">
                            <TabItem Header="Загальне опитування" Name="InterviewGuestReestr" Height="50" Width="160" HorizontalAlignment="Center" Margin="0,-5,0,0">
                                <Grid Width="630" x:Name="GuestGrid" Background="White" HorizontalAlignment="Left">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="40*" />
                                        <RowDefinition Height="520*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Width="630">
                                        <Border  BorderBrush="#FF858585" Background="White" BorderThickness="1,1,1,1"  Padding="1,1,1,1" Height="30" VerticalAlignment="Center" Width="630" >
                                            <DockPanel  >
                                                <Label Content=" Протокол опитування"  FontSize="14" FontWeight="Bold"  Height="28" Width="180" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,0,0,0" />
                                                <Label Content=" Назва опитування:"  FontSize="14"   Height="28" Width="140" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,0,0,0" />
                                                <TextBox x:Name="NameInterv" Text=""   Background="#FFDCDBDB" Width="270" Height="24" FontSize="14" HorizontalAlignment="Right" BorderThickness="0,0,0,0" />
                                            </DockPanel>
                                        </Border>
                                    </Grid>
                                    <Grid Grid.Row="1" Width="630">
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30*" />
                                                <RowDefinition Height="520*"/>
                                            </Grid.RowDefinitions>
                                        </Grid>


                                        <Border  BorderBrush="#FFB6B3B3" Background="#FF858585" BorderThickness="0,0,0,0"  Padding="1,1,1,1" Height="500" VerticalAlignment="Center" Width="630" >

                                            <ListView Grid.Row="1" Name="TablGuestInterviews" ItemsSource="{Binding GuestIntervs}"  VerticalAlignment="Center"
                                        SelectedItem ="{Binding SelectedGuestInterv}" Height="500" Background="White"
                                        Width="630" BorderThickness="1,1,1,1" ScrollViewer.HorizontalScrollBarVisibility="Hidden" 
                                        BorderBrush="Transparent" ScrollViewer.VerticalScrollBarVisibility="Auto">
                                                <ListView.ItemTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=detailsInterview}" TextWrapping="Wrap"  Width="600" Height="30" FontSize="14" FontWeight="Bold"   HorizontalAlignment="Left" />
                                                    </DataTemplate>
                                                </ListView.ItemTemplate>
                                            </ListView>
                                        </Border>
                                        <StackPanel x:Name ="StackPanelGuest" VerticalAlignment="Center" Height="300" Width="610" >
                                            <StackPanel.Effect>
                                                <DropShadowEffect Color="#FF1D1504" BlurRadius="35" ShadowDepth="20" Opacity="0.5" ></DropShadowEffect>
                                            </StackPanel.Effect>
                                            <Border x:Name="BorderGuest" Height="300" Width="600"  BorderBrush="#FFC8E0F7" Background="#FFC8E0F7"  VerticalAlignment="Center" HorizontalAlignment="Center"
                                                    BorderThickness="1,1,1,1"  CornerRadius="15,15,15,15" Cursor="Hand" >
                                                <Button  Command="{Binding AddCommand}" Background="#FFC8E0F7" BorderBrush="#FFC8E0F7"  Height="282" Width="580" VerticalAlignment="Center" HorizontalAlignment="Center" >
                                                    <Label x:Name="InputNameInterview" Content="
                                          Шановний користувач!&#xa;
                  З Вами спілкується комп'ютерна програма-SEAM.&#xa;&#xa;
 Вона допоможе  визначити, шляхом опитування, попередній діагноз &#xa;
                                            вашого нездужання.&#xa;
 Опитування містить:&#xa;      - визначення того що це за нездужання (головна біль, закладеність &#xa;
       носу або горла, біль у серці або животі, нежить, кашель тощо);&#xa;
     - виявлення характеру та деталей прояву  вашого нездужання.&#xa;&#xa;
               Для початку опитування натисніть на кнопку 'Додати',&#xa; "
                                                FontSize="17" Background="#FFC8E0F7"  Height="282" Width="580" VerticalAlignment="Center" HorizontalAlignment="Center"  Visibility="Visible"/>
                                                </Button>
                                            </Border>
                                        </StackPanel>
                                    </Grid>

                                </Grid>

                            </TabItem>
                            <TabItem Header="Запис на прийом&#xa;     до лікаря" Name="ZapisLikarGuest" Height="50" Width="145" HorizontalAlignment="Center">
                                <Grid Width="630" Background= "White" HorizontalAlignment="Left" x:Name="GridZapisLikarGuest">
                                    <Border BorderBrush="#FF979595" BorderThickness="1,1,1,1" Height="310" VerticalAlignment="Top">
                                        <StackPanel x:Name="ReceptionLikarGuestMapOpis" Width="Auto"  Background="White" Height="300" DataContext="{Binding SelectReceptionLIkarGuest}">



                                            <Grid Width="Auto" Height="Auto">
                                                <Grid.ColumnDefinitions >
                                                    <ColumnDefinition Width="110*" />
                                                    <ColumnDefinition Width ="281*" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="30*" />
                                                    <RowDefinition Height="30*" />
                                                    <RowDefinition Height="30*"/>
                                                    <RowDefinition Height="25*"/>
                                                    <RowDefinition Height="24*"/>
                                                    <RowDefinition Height="24*"/>
                                                    <RowDefinition Height="24*"/>
                                                    <RowDefinition Height="24*"/>
                                                    <RowDefinition Height="40*"/>
                                                    <RowDefinition Height="80*"/>
                                                </Grid.RowDefinitions>
                                                <Label Content="     Запис на прийом   до лікаря" Grid.Row="1" Grid.Column="1"  Height="26" FontWeight="Bold" />
                                                <TextBlock x:Name="ReceptionLikarGuestLab1" Grid.Column="0" Grid.Row="5" Text="  Дата та час  опитування"  FontSize="14" Height="24" Width="170" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                                <TextBox Text="{Binding dateInterview,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest1" Grid.Row="5" Background="White" Width="150" FontSize="14" HorizontalAlignment="Left" BorderThickness="0,0,0,0" Grid.ColumnSpan="2" Margin="179,0,0,0"/>
                                                <TextBlock x:Name="ReceptionLikarGuestLab2" Grid.Row="3" TextWrapping="Wrap"  Grid.Column="0" Height="24" Text="  Лікар"   Width="150" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="14"  />
                                                <TextBox Text="{Binding nameDoctor,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest2" Grid.Row="3" Grid.Column="1" FontSize="14" Height="24" Background="White" VerticalContentAlignment="Center" Width="380" HorizontalAlignment="Left" BorderThickness="0,0,0,0"/>
                                                <Image x:Name="ReceptionLikarFolderLikarGuest" Source="pack://application:,,,/FrontSeam;component/Images/folder_.png" Grid.Column="1" Grid.Row="3" Width="25"  Margin="380,0,0,0" Visibility="Hidden"  >
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="MouseLeftButtonDown">
                                                            <i:InvokeCommandAction Command="{Binding ElementName=GridZapisLikarGuest, Path=DataContext.ReseptionGuestLikars}" CommandParameter=""/>
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </Image>


                                                <TextBlock x:Name="ReceptionLikarGuestLab3" Grid.Column="0" Grid.Row="2" Text="  Пацієнт (Ім'я,&#xa;  Прізвище, телефон)" TextWrapping="Wrap"  FontSize="14" Height="32" Width="170" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                                <!--<TextBlock x:Name="ReceptionLikarGuestAddCompInterview" Grid.Row="2" Grid.Column="1" Text=" Додати інтерв'ю"  FontSize="14" Height="22" Width="110" Margin="290,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"   />
                                                <Image x:Name="ReceptionLikarGuestAddInterv" Source="pack://application:,,,/FrontSeam;component/Images/folder_.png" Grid.Column="1" Grid.Row="2" Width="25" Height="24" Margin="380,0,0,0" Visibility="Visible"  >
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="MouseLeftButtonDown">
                                                            <i:InvokeCommandAction Command="{Binding ElementName=GridCompletedInterview, Path=DataContext.ReadColectionIntevLikars}" CommandParameter=""/>
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </Image>-->
                                                <TextBox Text="{Binding namePacient,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest3" Grid.Column="1" Grid.Row="2" Background="White" Width="280" Height="24" FontSize="14" HorizontalAlignment="Left" BorderThickness="0,0,0,0" />



                                                <TextBlock x:Name="ReceptionLikarGuestLab4" Grid.Column="0" Grid.Row="4" Text="  Дата та час прийому"  FontSize="14" Height="24" Width="160" HorizontalAlignment="Left"  />
                                                <Image x:Name="ReceptionLikarFolderGuestTime" Source="pack://application:,,,/FrontSeam;component/Images/folder_.png" Grid.Column="1" Grid.Row="4" Width="25"  Margin="380,0,0,0" Visibility="Hidden"  >
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="MouseLeftButtonDown">
                                                            <i:InvokeCommandAction Command="{Binding ElementName=GridZapisLikarGuest, Path=DataContext.SelectDaysVisiting}" CommandParameter=""/>
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </Image>
                                                <TextBox Text="{Binding dateDoctor,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest4" Grid.Column="1" Grid.Row="4"  Background="White" Width="200" Height="24" FontSize="14" HorizontalAlignment="Left" BorderThickness="0,0,0,0" />

                                                <TextBlock x:Name="ReceptionLikarGuestLab5" Grid.Column="0" Grid.Row="7" Text="  Рекомендації" TextWrapping="Wrap"  FontSize="14" Height="24" Width="165" HorizontalAlignment="Left" VerticalAlignment="Bottom" />
                                                <TextBox Text="{Binding nameRecomen,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest5" Grid.Column="1" Grid.Row="7" Background="White" Width="400" Height="24" FontSize="14" HorizontalAlignment="Left" BorderThickness="0,0,0,0" />

                                                <TextBlock x:Name="ReceptionLikarGuestLab6" Grid.Column="0" Grid.Row="6" Text="  Попередній діагноз"  FontSize="14" Height="32" Width="160" HorizontalAlignment="Left"  />
                                                <TextBox Text="{Binding nameDiagnoz,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest6" Grid.Column="1" Grid.Row="6"  Background="White" Width="400" Height="24" FontSize="14" HorizontalAlignment="Left" BorderThickness="0,0,0,0" />


                                                <TextBlock x:Name="ReceptionLikarGuestLab7" Grid.Column="0" Grid.Row="8" Text="  Зміст звернення"  FontSize="14" Height="24" Width="146" HorizontalAlignment="Left"  />
                                                <TextBox Text="{Binding resultDiagnoz,UpdateSourceTrigger=PropertyChanged}" Name="ReceptionLikarGuest7" Grid.Column="1" Grid.Row="8"  Background="White" Width="400" Height="24" FontSize="14"
                                                    IsEnabled="False" HorizontalAlignment="Left" BorderThickness="0,0,0,0" />
                                                <Label Name="ReceptionLikarGuestSp" Content="" Grid.Row="8"></Label>

                                                <TextBlock x:Name="ReceptionLikarGuestCompInterview" Grid.Row="5" Grid.Column="1" Text=" Зміст опитування"  FontSize="14" Height="22" Width="120" Margin="270,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Visibility="Hidden"  />
                                                <Image x:Name="ReceptionLikarGuestFoldInterv" Source="pack://application:,,,/FrontSeam;component/Images/folder_.png" Grid.Column="1" Grid.Row="5" Width="25" Height="24" Margin="380,0,0,0" Visibility="Hidden"  >
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="MouseLeftButtonDown">
                                                            <i:InvokeCommandAction Command="{Binding ElementName=GridZapisLikarGuest, Path=DataContext.ReadColectionIntreview}" CommandParameter=""/>
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </Image>
                                                <TextBlock Name="ReseptionZapisLikar" Text="Для запису на прийом необхідно виконати опитування, потім перейти на цю закладку , вибрати лікаря, ввести дані та натиснути на кнопк 'Зберегти'. " Width="630"  Height="70"
                                                   TextWrapping="Wrap" Grid.Row="9" HorizontalAlignment="Center" FontSize="18"  Grid.ColumnSpan="2" Margin="0,0,0,0"/>
                                            </Grid>

                                        </StackPanel>
                                    </Border>
                                </Grid>

                            </TabItem>

                        </TabControl>
                    </TabItem>
                    <TabItem Header="   Кабінет пацієнта" Name="Pacient"   FontFamily="Microsoft Sans Serif" Background="White"
                             FontSize="14" Height="50" Width="150" BorderThickness="0,0,0,0" >
                        <TabControl x:Name="ControlPacient"  HorizontalAlignment="Left" Height="550" VerticalAlignment="Top"  Width="800"  BorderThickness="0,0,0,0" TabStripPlacement="Left">
                            <TabItem Header="Профіль пацієнта" Name="PacientProfil" Height="50" Width="150" HorizontalAlignment="Center" Margin="0,-5,0,0">
                                <Grid Width="640" Height="550" Background="White" HorizontalAlignment="Left">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="390*"/>
                                        <RowDefinition Height="260*"/>
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" BorderBrush="#FFB6B3B3" Background="#FF858585"  BorderThickness="1" Height="300" Width="630" Padding="0,0,0,0" VerticalAlignment="Top">
                                        <StackPanel x:Name="PacientPanelProfil" Width="Auto" Background="White" Height="300" DataContext="{Binding SelectedPacientProfil}" >
                                            <Grid Width="Auto" Height="Auto">
                                                <Grid.ColumnDefinitions >
                                                    <ColumnDefinition Width ="100*" />
                                                    <ColumnDefinition Width ="125*" />
                                                    <ColumnDefinition Width="89*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="24*" />
                                                    <RowDefinition Height="50*" />
                                                    <RowDefinition Height="50*"/>
                                                    <RowDefinition Height="50*"/>
                                                    <RowDefinition Height="50*" />
                                                    <RowDefinition Height="50*"/>
                                                    <RowDefinition Height="50*"/>

                                                </Grid.RowDefinitions>
                                                <Grid Grid.Column="1" Grid.Row="0" >
                                                    <Label Content="     Профіль пацієнта"   Height="26" FontWeight="Bold" />
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab1"  Grid.Column="0" Grid.Row="1" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width="60*"/>
                                                        <ColumnDefinition Width ="92*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab1" Grid.Column="0"  Text="  ID код" TextWrapping="Wrap"  FontSize="14" Width="60" Height="24" HorizontalAlignment="Left" />
                                                    <TextBox Text="{Binding kodPacient, UpdateSourceTrigger=PropertyChanged}" Name="PacientProfilt1" Grid.Column="1" IsEnabled="False" Height="24" Width="120" FontSize="14" Background="White" VerticalAlignment="Center" HorizontalAlignment="Left" BorderThickness="0,0,0,0"  />
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab2" Height="50" Grid.Column="0" Grid.Row="2" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width ="60*" />
                                                        <ColumnDefinition Width ="95*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab2" Grid.Column="0" Height="22" Text="  Ім'я" TextWrapping="Wrap" Width="60" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="14"   />
                                                    <TextBox Text="{Binding name, UpdateSourceTrigger=PropertyChanged}" Name="PacientProfilt2" IsEnabled="False" FontSize="14" Grid.Column="1"  Height="24" Width="120" Background="White" VerticalContentAlignment="Center" HorizontalAlignment="Left" BorderThickness="0,0,0,0" Margin="2,13,0,13"/>
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab3" Height="50" Grid.Column="0" Grid.Row="3" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width ="60*" />
                                                        <ColumnDefinition Width ="90*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab3" Text="  Прізвище" TextWrapping="Wrap" Grid.Column="0"  Height="24"  FontSize="14" Width="80" HorizontalAlignment="Left" />
                                                    <TextBox Text="{Binding surname, UpdateSourceTrigger=PropertyChanged}" x:Name="PacientProfilt3" IsEnabled="False" FontSize="14" Grid.Column="1" Height="24" Width="140" VerticalAlignment="Center" HorizontalAlignment="Left" Background="White" BorderThickness="0,0,0,0" Margin="1,0,0,0" />
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab4" Height="50" Grid.Column="0" Grid.Row="4" >
                                                    <Grid.ColumnDefinitions >

                                                        <ColumnDefinition Width="60*"/>
                                                        <ColumnDefinition Width ="90*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab4" Grid.Column="0"  Text="  Вік" TextWrapping="Wrap"  FontSize="14" Height="24" Width="70" HorizontalAlignment="Left" Margin="0,13,0,0" VerticalAlignment="Top"/>
                                                    <TextBox Text="{Binding age, UpdateSourceTrigger=PropertyChanged}" Name="PacientProfilt4" IsEnabled="False" FontSize="14" Grid.Column="1"  Height="24" Width="100" VerticalAlignment="Center" Background="White" BorderThickness="0,0,0,0" HorizontalAlignment="Left" Margin="1,0,0,0"   />
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab5" Height="50" Grid.Column="1" Grid.Row="1" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width ="70*" />
                                                        <ColumnDefinition Width ="90*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab5" Text="  Вага(кг.)" TextWrapping="Wrap" Grid.Column="0" Height="24" Width="84" HorizontalAlignment="Left" FontSize="14" Margin="0,10,0,10" />
                                                    <TextBox Text="{Binding weight, UpdateSourceTrigger=PropertyChanged}" IsEnabled="False" x:Name="PacientProfilt5" Grid.Column="1"  Width="80"  Background="White" HorizontalAlignment="Left" Height="24" VerticalAlignment="Center" FontSize="14" BorderThickness="0,0,0,0" Margin="1,0,0,0"  />
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab6" Height="50" Grid.Column="1" Grid.Row="2" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width ="70*" />
                                                        <ColumnDefinition Width ="90*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab6" Text="  Зріст(см)" TextWrapping="Wrap"  Grid.Column="0" Height="24"  FontSize="14" HorizontalAlignment="Left" Width="70"/>
                                                    <TextBox Text="{Binding growth, UpdateSourceTrigger=PropertyChanged}" x:Name="PacientProfilt6" IsEnabled="False" Grid.Column="1"  Background="White" Height="24" Width="80" FontSize="14" VerticalAlignment="Center" HorizontalAlignment="Left" BorderThickness="0,0,0,0" Margin="1,0,0,0" />
                                                </Grid>
                                                <Grid x:Name="PacientProfilGridLab7" Height="50" Grid.Column="1" Grid.Row="3" >
                                                    <Grid.ColumnDefinitions >
                                                        <ColumnDefinition Width ="70*" />
                                                        <ColumnDefinition Width ="90*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock x:Name="PacientProfilLab7" Text="  Стать" TextWrapping="Wrap" Grid.Column="0" Height="24"  FontSize="14" Width="60" HorizontalAlignment="Left"/>
